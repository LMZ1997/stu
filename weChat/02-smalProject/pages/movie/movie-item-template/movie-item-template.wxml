<import src="../star-template/star-template.wxml" />
<template name="movieItemTemplate">
  <view class='movie-item'>
    <image src='{{image}}' class='movie-image'></image>
    <text class='title'>{{title}}</text>
    <template is="starTemplate" data="{{stars:stars,score:score}}" />
  </view>
</template>